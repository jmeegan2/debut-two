<div class="page-width">
    {% if section.settings.title != blank %}
      <div class="section-header text-center">
        <h2>{{ section.settings.title | escape }}</h2>
      </div>
    {% endif %}
   
    {% for member in section.blocks %}
      <div>
          <h3>{{ member.settings.name }}</h3>
          <span>{{ member.settings.title }}</span>
          <p>{{ member.settings.bio }}</p>
      </div>

      {% endfor %}

</div>

{% schema %}
{
    "name" :  "Team Members",
    "class" : "team-members-section",
    "settings" : [
    {
        "id" : "title",
        "type" : "text",
        "label" : "Section Title",
        "default" : "Meet ou iiiiiiiTeam"
    }
    ]
    "blocks" : [ 
    {
      "name" : "Team member",
      "type" : "staff-profile",
      "settings" : [
      {
        "id" : "name",
        "type" : "text",
        "label" : "Name"
        
      },
      {
        "id" : "title",
        "type" : "text",
        "label" : "Job Title"
      },
      {
        "id" : "bio",
        "type" : "textarea",
        "label" : "Bio"
      }
      ]
    }
    ]
}
{% endschema %}
