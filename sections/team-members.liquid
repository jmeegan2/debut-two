<div class="page-width">
    {% if section.settings.title != blank %}
      <div class="section-header text-center">
        <h2>{{ section.settings.title | escape }}</h2>
      </div>
    {% endif %}
</div>

{% schema %}
{
    "name" :  "Team Members",
    "class" : "team-members-section",
    "settings" : [
    {
        "id" : "title",
        "type" : "text",
        "label" : "Secton Title",
        "default" : "Meet our Team"
    }
    ],
    "blocks" : [ 
    {
      "name" : "Team member",
      "type" : "staff-profile",
      "settings" : [
      {
        "id" : "staff-name",
        "type" : "text",
        "label" : "Name"
        
      }
      ]
    }
    ]
}
{% endschema %}
